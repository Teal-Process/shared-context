{
    "name": "Shared Context",
    "version": "0.1",
    "description": "A tool to start tangibly playing with the idea of digital proximity",
    "permissions": [
		"activeTab"
    ],
    "content_scripts": [
		{
			"matches": ["<all_urls>"],
            "css": [ "context.css" ],
			"js": ["bundle.js"]
		}
	],
    "manifest_version": 2
  }